generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

enum MenuVersionStatus {
  DRAFT
  PUBLISHED
  REPLACED
}

model MenuVersion {
  id        String            @id @default(cuid())
  status    MenuVersionStatus @default(DRAFT)
  createdAt DateTime          @default(now())
  updatedAt DateTime          @updatedAt
}
